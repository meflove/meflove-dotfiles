{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "padding": {
      "top": 1,
      "left": 2,
    },
    "source": "arch",
    "color": {
      "1": "blue",
      "2": "magenta",
    },
  },
  "modules": [
    {
      "type": "os",
      "key": "╭─󰣇", // Just get your distro's logo off nerdfonts.com
      "keyColor": "blue",
    },
    {
      "type": "kernel",
      "key": "├─",
      "keyColor": "blue",
    },
    {
      "type": "uptime",
      "key": "├─󰅐",
      "keyColor": "blue",
    },
    {
      "type": "media",
      "key": "╰─󰝚",
      "keyColor": "blue",
    },
    "break",
    {
      "type": "packages",
      "key": "╭─󰏖",
      "keyColor": "red",
    },
    {
      "type": "shell",
      "key": "├─",
      "keyColor": "red",
    },
    {
      "type": "display",
      "key": "├─󰍹",
      "keyColor": "red",
    },
    "de",
    {
      "type": "wm",
      "key": "├─",
      "keyColor": "red",
    },
    {
      "type": "theme",
      "key": "├─󰉼",
      "keyColor": "red",
    },
    {
      "type": "icons",
      "key": "├─󰀻",
      "keyColor": "red",
    },
    {
      "type": "font",
      "key": "├─",
      "keyColor": "red",
    },
    {
      "type": "terminal",
      "key": "╰─",
      "keyColor": "red",
      "format": "{pretty-name} {version} {#37}█{#97}█ {#36}█{#96}█ {#35}█{#95}█ {#34}█{#94}█ {#33}█{#93}█ {#32}█{#92}█ {#31}█{#91}█ {#30}█{#90}█",
    },
    "break",
    {
      "type": "cpu",
      "format": "{name}",
      "showPeCoreCount": true,
      "temp": true,
      "key": "╭─󰻠",
      "keyColor": "magenta",
    },
    {
      "type": "gpu",
      "format": "{name}",
      "temp": true,
      "key": "├─󰍛",
      "keyColor": "magenta",
    },
    {
      "type": "memory",
      "key": "├─󰑭",
      "keyColor": "magenta",
    },
    {
      "type": "disk",
      "key": "├─",
      "keyColor": "magenta",
    },
    // {
    //   "type": "publicip",
    // "key": "├─󰩠",
    // "keyColor": "magenta",
    // "timeout": 2000
    // },
    {
      "type": "localip",
      "key": "├─󰩟",
      "compact": true,
      "keyColor": "magenta",
    },
    {
      "type": "wifi",
      "key": "╰─",
      "format": "{ssid}",
      "keyColor": "magenta",
    },
  ],
}
